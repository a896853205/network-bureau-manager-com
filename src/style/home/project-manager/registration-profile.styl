.registration-profile-box {
  display: flex;
  align-items: center;
  justify-content: space-between;

  .profile-left-box {
    width: calc(98% - 300px);

    .left-item-box {
      display: flex;
      border-style: solid;
      border-width: 1px;
      padding: 10px;

      .item-icon-box {
        font-size: 75px;
        width: 30%;
      }

      .item-text-box {
        display: flex;
        flex-direction: column;
        width: 70%;

        .text-top-box {
          display: flex;
          justify-content: center;
          font-size: 16px;
          font-weight: 600;
        }

        .text-row-box {
          display: flex;
          justify-content: space-between;
          font-size: 14px;
        }
      }
    }
  }

  .profile-right-box {
    width: 300px;

    .enterprise-info-bottom-box {
      display: flex;
      flex-direction: column;
      align-items: center;
      padding-left: 0;

      .enterprise-info-item-box {
        width: 80%;
        display: flex;
        padding: 6px 0;
        justify-content: space-between;

        .enterprise-info-icon {
          margin-right: 2px;
          font-size: 18px;
        }
      }
    }
  }
}